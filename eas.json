{
  "cli": {
    "version": ">= 16.13.2",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_API_BASE": "http://192.168.0.35:8000",
        "EXPO_PUBLIC_WS_BASE": "ws://192.168.0.35:8000/ws/chat",
        "EXPO_PUBLIC_DEV_HOST": "192.168.0.35"
      }
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "EXPO_PUBLIC_API_BASE": "https://api.haniumpicky.click",
        "EXPO_PUBLIC_WS_BASE": "wss://api.haniumpicky.click/wss/chat"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
